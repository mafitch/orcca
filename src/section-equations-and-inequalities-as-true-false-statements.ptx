Context()-&gt;noreduce('(-x)-y');
$yes=PopUp(['?','is','is not'],1);
$no=PopUp(['?','is','is not'],2);
@allvars=('x','y','z','r','q','p','V');
@var=(
list_random(@allvars),
(@allvars)[NchooseK(7,2)],
list_random(@allvars),
list_random(@allvars),
list_random(@allvars),
'r',
);
@left=(
Formula(non_zero_random(-9,9,1).$var[0].'+'.non_zero_random(-19,19,1))-&gt;reduce,
