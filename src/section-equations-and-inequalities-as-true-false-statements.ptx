Formula(non_zero_random(-9,9,1).'+'.non_zero_random(-9,9,1).$var[3].'^2')-&gt;reduce,
Formula(non_zero_random(-9,9,0.01).$var[4])-&gt;reduce,
Formula('sqrt('.non_zero_random(-9,9,0.1).$var[5].'+'.non_zero_random(-9,9,1).')')-&gt;reduce,
Formula("2 pi r")-&gt;reduce
);
@right=(
Compute(random(-9,9,1)),
Compute(random(-29,29,1)),
Compute(random(-29,29,1)),
Compute(random(-9,9,1)),
Compute(random(0,9,1)),
Formula(random(1,20,1).'pi')-&gt;reduce
);
@eq = map{$left[$_]-&gt;TeX.'='.$right[$_]-&gt;TeX}(0..5);
@answer = (
$yes,
$no,
$no,
$yes,
