    <exercisegroup cols="2" permid="pIv">
      <title>Set Notation</title>
      <exercise permid="XER">
        <webwork>
        <setup>
<pg-code>
$a=random(1,9,1);
$a=2 if ($envir{problemSeed}==1);
$b=$a**2;
Context("Interval");
$set=Set("{-$a,$a}");
$setev=$set-&gt;cmp(cmp_class=&gt;'a set');
Context()-&gt;parens-&gt;set('('=&gt;{type=&gt;'Point'});
</pg-code>
        </setup>
        <statement>
