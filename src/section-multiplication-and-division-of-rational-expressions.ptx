    <exercisegroup permid="Hon">
      <title>Challenge</title>
      <exercise permid="WbM">
<!-- Kara's challenge problem -->
        <webwork>
            <setup>
<pg-code>
$denom = random(25,95,5);
$minusone = $denom - 1;
$answer = Formula("1/(x+$denom)");
</pg-code>
            </setup>
            <statement>
              <p permid="stK">
                Simplify the following:
                <m>\frac{1}{x+1} \div \frac{x+2}{x+1} \div \frac{x+3}{x+2} \div \frac{x+4}{x+3} \div \dots \frac{x+<var name="$denom"/>}{x+<var name="$minusone"/>} </m>.
                For this exercise,
                you do not have to write the restricted domain of the simplified expression.
              </p>
              <p permid="YAT">
                <var name="$answer" width="10"/>
              </p>
            </statement>
            <solution permid="UWt">
              <p permid="eqf">
                <md permid="eZZ">
                  <mrow>\amp\frac{1}{x+1} \div \frac{x+2}{x+1} \div \frac{x+3}{x+2} \div \frac{x+4}{x+3} \div \dots \frac{x+<var name="$denom"/>}{x+<var name="$minusone"/>}</mrow>
                  <mrow>\amp=\frac{1}{x+1} \cdot \frac{x+1}{x+2} \cdot \frac{x+2}{x+3} \cdot \frac{x+3}{x+4} \cdot \dots \frac{x+<var name="$minusone"/>}{x+<var name="$denom"/>}</mrow>
                  <mrow>\amp=\frac{1}{x+<var name="$denom"/>}, x \neq 1, 2, 3, \dots <var name="$minusone"/></mrow>
                </md>
              </p>
            </solution>
        </webwork>
